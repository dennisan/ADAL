@model MultiTenantWithRead.Models.UserProfile
@{
    ViewBag.Title = "User Profile";
}

<h2>@ViewBag.Title.</h2>


<div class="row">
	<div class="col-md-2">
		<img class="img-thumbnail" src="@Model.Country" />

	</div>

	<div class="col-md-10">
		<table class="table table-bordered table-striped">
			<tr>
				<td>Display Name</td>
				<td>@Model.DisplayName</td>
			</tr>
			<tr>
				<td>First Name</td>
				<td>@Model.GivenName</td>
			</tr>
			<tr>
				<td>Last Name</td>
				<td>@Model.Surname</td>
			</tr>
			<tr>
				<td>Street</td>
				<td>@Model.StreetAddress</td>
			</tr>
			<tr>
				<td>City</td>
				<td>@Model.City</td>
			</tr>
			<tr>
				<td>State</td>
				<td>@Model.State</td>
			</tr>
			<tr>
				<td>Zip</td>
				<td>@Model.PostalCode</td>
			</tr>
		</table>
	</div>

</div>
